#include<stdio.h>
int fibo(int num)
{
	if(num==0)
	{
		return 0;
	}
	else if(num==1)
	{
		return 1;
	}
	else
	{
		return (fibo(num-1)+fibo(num-2));
	}
}
int main()
{
	int num,result,i;
	printf("Rohith Krishna,192210092\n");
	printf("enter n value:");
	scanf("%d",&num);
	for(i=0;i<num;i++)
	{
		printf("%d ",fibo(i));
	}
}
